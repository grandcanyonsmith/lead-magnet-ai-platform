{
  "name": "leadmagnet-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p ${PORT:-3000} -H 0.0.0.0",
    "clean:dev": "node -e \"require('fs').rmSync('.next-dev',{recursive:true,force:true})\"",
    "dev:clean": "npm run clean:dev && npm run dev",
    "build": "next build",
    "start": "next start -p 5000 -H 0.0.0.0",
    "start:deploy": "next start -p ${PORT:-80} -H 0.0.0.0",
    "lint": "next lint",
    "export": "next export",
    "postinstall": "npx browserslist@latest --update-db || true",
    "test:e2e": "playwright test -c e2e/playwright.config.ts --project=chromium",
    "test:e2e:all": "playwright test -c e2e/playwright.config.ts",
    "test:e2e:ui": "playwright test -c e2e/playwright.config.ts --project=chromium --ui",
    "test:e2e:headed": "playwright test -c e2e/playwright.config.ts --project=chromium --headed",
    "test:e2e:debug": "playwright test -c e2e/playwright.config.ts --project=chromium --debug"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^3.3.4",
    "@stripe/stripe-js": "^2.4.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-query": "^5.90.8",
    "@types/react-syntax-highlighter": "^15.5.13",
    "amazon-cognito-identity-js": "^6.3.8",
    "axios": "^1.6.5",
    "client-only": "^0.0.1",
    "clsx": "^2.1.0",
    "date-fns": "^3.2.0",
    "lucide-react": "^0.309.0",
    "nanoid": "^5.0.0",
    "next": "^14.2.33",
    "next-themes": "^0.2.1",
    "picocolors": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-joyride": "^2.5.2",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "react-webcam": "^7.2.0",
    "reactflow": "^11.11.4",
    "recharts": "^2.10.3",
    "remark-gfm": "^4.0.1",
    "source-map-js": "^1.2.1",
    "styled-jsx": "5.1.1",
    "tailwind-merge": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "autoprefixer": "^10.4.17",
    "browserslist": "^4.23.0",
    "caniuse-lite": "^1.0.30001520",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}

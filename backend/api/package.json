{
  "name": "leadmagnet-api",
  "version": "1.0.0",
  "description": "API Lambda functions for lead magnet platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "postbuild": "tsc-alias",
    "watch": "npm run watch:tsc & npm run watch:aliases",
    "watch:tsc": "tsc -w",
    "watch:aliases": "tsc-alias -w",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "package": "npm run build && cd dist && zip -r ../api.zip .",
    "package:lambda": "npm run build && mkdir -p bundle && npx esbuild dist/index.js --bundle --platform=node --target=node20 --format=cjs --outfile=bundle/index.js && cd bundle && zip -r ../api-bundle.zip .",
    "dev": "npm run build && node server-local.js",
    "dev:watch": "npm run watch & npm run dev"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.470.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.470.0",
    "@aws-sdk/client-dynamodb": "^3.470.0",
    "@aws-sdk/client-ecs": "^3.470.0",
    "@aws-sdk/client-lambda": "^3.922.0",
    "@aws-sdk/client-s3": "^3.470.0",
    "@aws-sdk/client-secrets-manager": "^3.470.0",
    "@aws-sdk/client-sfn": "^3.470.0",
    "@aws-sdk/lib-dynamodb": "^3.470.0",
    "@aws-sdk/s3-request-presigner": "^3.911.0",
    "@aws-sdk/util-dynamodb": "^3.470.0",
    "aws-lambda": "^1.0.7",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "openai": "^4.100.0",
    "stripe": "^14.0.0",
    "ulid": "^2.4.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.19.24",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "tsc-alias": "^1.8.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}